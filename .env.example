
APP_NAME=sfood
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Map Configuration
# Supported drivers: "google", "iran"
MAP_DRIVER=iran
# Default center coordinates for maps
MAP_DEFAULT_LAT=35.6892
MAP_DEFAULT_LNG=51.3890
MAP_DEFAULT_ZOOM=12

# Map API Keys
# Used by GoogleMapProvider
MAP_API_KEY=
MAP_API_KEY_SERVER=
# Used by IranMapProvider (Neshan)
NESHAN_API_KEY=

# Tile Server Configuration for Leaflet
# Primary tile server URL template
MAP_TILE_URL="https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"
# Pipe-separated fallback tile server URLs, e.g., "https://tile-a.url|https://tile-b.url"
MAP_TILE_URLS=

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=sfood
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
